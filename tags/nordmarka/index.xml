    <item>
      <title>Maridalen</title>
      <link>https://erikjohannes.no/posts/20190310-maridalen/</link>
      <pubDate>Sun, 10 Mar 2019 13:27:23 +0200</pubDate>
      
      <guid>https://erikjohannes.no/posts/20190310-maridalen/</guid>
      <description>&lt;link rel=&#34;stylesheet&#34; href=&#34;https://erikjohannes.no/js/leaflet/leaflet.css&#34; /&gt;
&lt;script src=&#34;https://erikjohannes.no/js/leaflet/leaflet.js&#34;&gt;&lt;/script&gt;
&lt;script src=&#34;https://erikjohannes.no/js/gpx.js&#34;&gt;&lt;/script&gt;

&lt;div id=&#34;map&#34; style=&#34;height: 400px; width: 100%;&#34;&gt;&lt;/div&gt;

&lt;script&gt;
        var map = L.map(&#39;map&#39;);//.setView([60.14, 10.25], 11);
        L.tileLayer(&#39;http://opencache.statkart.no/gatekeeper/gk/gk.open_gmaps?layers=topo4&amp;zoom={z}&amp;x={x}&amp;y={y}&#39;, {
            attribution: &#39;&lt;a href=&#34;http://www.kartverket.no/&#34;&gt;Kartverket&lt;/a&gt;&#39;
        }).addTo(map);
        var gpx = &#39;/posts/20190310-maridalen/h√∏gruta-i-maridalen.gpx&#39;; 
        new L.GPX(gpx, {
            async: true,
            marker_options: {
                startIconUrl: &#39;../images/pin-icon-start.png&#39;,
                endIconUrl:   &#39;../images/pin-icon-end.png&#39;,
                shadowUrl:    &#39;../images/pin-shadow.png&#39;,
                //clickable: true,
                //showRouteInfo: true
            },
        }).on(&#39;loaded&#39;, function(e) {
            map.fitBounds(e.target.getBounds());
        }).addTo(map);
&lt;/script&gt;


&lt;p&gt;&lt;img src=&#34;https://erikjohannes.no/posts/20190310-maridalen/maridalen01.jpg&#34; alt=&#34;&#34;&gt;
